\DeclareOptionX{violet}{\def\secondaryColorName{Violet}}
\DeclareOptionX{Orange}{\def\secondaryColorName{Orange}}
\DeclareOptionX{Green}{\def\secondaryColorName{Green}}
\DeclareOptionX{Blue}{\def\secondaryColorName{Blue}}
\DeclareOptionX{Violet}{\def\secondaryColorName{Violet}}
