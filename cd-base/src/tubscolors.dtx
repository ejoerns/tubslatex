%    \begin{key}{}{trueblack}
%    \begin{key}{}{richblack}
% Stellt CMYK-Schwarz als 'richtiges' Schwarz dar, also nicht als
% 100%K-Farbe, sondern als CMYK-Farbmischung.
% Die Stanardmischung ist: C:75\%, M:68\%, Y:67\%, K:90\%.
% Diese Option zeigt nur Auswirkung in Verbindung mit der Option |cmyk|.
%    \begin{macrocode}
\def\tc@trueblackval{0.75,0.68,0.67,0.90}%
\DeclareOptionX{trueblack}[0.75,0.68,0.67,0.90]{%
  \tc@trueblacktrue
  \def\tc@trueblackval{#1}
}
\DeclareOptionX{richblack}[0.75,0.68,0.67,0.90]{%
  \tc@trueblacktrue
  \def\tc@trueblackval{#1}
}
%    \end{macrocode}
%    \end{key}\end{key}
